{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[css]": {
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
  },
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.rulers": [
    {
      "color": "#fd3e3e78",
      "column": 120
    }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.exclude": {
    "**/*.lock.json": false,
    "**/*?.d.ts": false,
    "lib/**/*?.js": false,
    "**/.DS_Store": false,
    "**/.git": false,
    "**/.gradle/**": false,
    "**/.idea/**": false,
    "**/.svn": false,
    "**/.tmp": false,
    "**/Pods/**": false,
    "**/bin/**": false,
    "**/build/**": false,
    "**/dist/**": false,
    "**/node_modules": false,
    "**/obj/**": false,
    "**/vendor/**": false,
    ".docker-data": false,
    "tsconfig.tsbuildinfo": false
  }
}